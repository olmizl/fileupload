<%- include("include/header"); %>
<main id="main">
  <div id="contents">
    <div class="btns_list">
      <a href="/insert">insert</a>
    </div>

    <h2 class="title"><%= title %></h2>
    <!-- 서버로 파일 전송할때 form태그안에 enctype="multipart/form-data" 필수 -->
    <div class="table">
      <div class="head">
        <span style="width: 10%">NO</span>
        <span>Subject</span>
        <span style="width: 20%">Date</span>
      </div>
      <ul class="body">
        <% list.forEach((item, idx)=> { %>
        <li>
          <div class="column"><%= (idx+1) %></div>
          <div class="column subject"><a href="/movie/<%= item.movieTitle %>"><%= item.movieTitle %></a></div>
          <div class="column"><%= item.date %></div>
        </li>
        <% }) %>
        <li></li>
      </ul>
    </div>
  </div>
</main>
<script src="//t1.daumcdn.net/mapjsapi/bundle/postcode/prod/postcode.v2.js"></script>

<script></script>
<%- include("include/footer"); %>
